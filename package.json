{
  "name": "testing-hooks-example",
  "private": true,
  "version": "1.0.0",
  "title": "Testing Javascript",
  "description": "Learn how to test js files and react components",
  "main": "index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "Tatiana Klianchina",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "coverage",
    "node_modules",
    "build",
    "other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klianchina/testing-hooks-example.git"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "import-all.macro": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "eslint": "8.0.1",
    "jest": "27.3.1",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "1.0.0",
    "react-scripts": "4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
